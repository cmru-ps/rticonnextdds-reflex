	<xs:element name="DataRef" type="DataRef"/>
	<xs:complexType name="DataRef">
		<xs:annotation>
			<xs:documentation>A reference to one data item. It is essentially a descriptor to one DataEvent value. (Explanation uses this as a type of pointer.)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dataIdType" type="DataIdType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specifies what id is meant to be a reference to (measurement location or agent).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataType" type="OsacbmDataType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specifies the type of data.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="xs:unsignedInt" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A MIMOSA meas_loc_id or agent_id.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="itemId" type="ItemId" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>References the segment or asset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="site" type="Site" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Contains the associated site information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="time" type="OsacbmTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Stores the associated time.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
